{
    "name": "gabrielelana/precious",
    "description": "Library to build value objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "gabriele.lana",
            "email": "gabriele.lana@cleancode.it"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Precious\\": ["./src", "./tests"]
        }
