    "suggest": {
        "phpstan/phpstan": "^0.11",
        "nikic/php-parser": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11",
        "nikic/php-parser": "^4.1"
    }
}
